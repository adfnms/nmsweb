<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="com.bluecapsystem.nms.service">

	<select id="getServicesName" parameterClass="java.util.Map" resultClass="com.bluecapsystem.nms.dto.ServicesCategoriesTbl">

	SELECT 
			serviceid 	AS 	serviceid, 
			servicename AS 	servicename
  	FROM 
			service;
		
	</select>
	
</sqlMap>
	
